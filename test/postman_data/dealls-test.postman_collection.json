					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 200) {\r",
							"    // Parse the response body\r",
							"    let responseData = pm.response.json();\r",
							"\r",
							"    // Check if the token exists\r",
							"    if (responseData.token) {\r",
							"        // Set the token to the environment variable\r",
							"        pm.environment.set(\"jwt_token\", responseData.token);\r",
							"        console.log(\"JWT token set to environment variable:\", responseData.token);\r",
							"    } else {\r",
							"        console.warn(\"Response does not contain a token.\");\r",
							"    }\r",
							"} else {\r",
							"    console.warn(\"Response status is not 200. Token not saved.\");\r",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
